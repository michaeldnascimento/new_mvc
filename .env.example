# URL PROJETO
URL=http://localhost/
# DADOS DE ACESSO DO BANCO MYSQL
DB_HOST=localhost
DB_USER=root
DB_PASS=root
DB_NAME=name_banco
# CASO DESEJE COLOCAR AS ROTAS EM MANUTENÇÃO (MAINTENANCE=true)
